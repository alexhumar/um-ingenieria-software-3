<p><h1 id="pac">{{paciente.apellido}}, {{paciente.nombre}}</h1></p>
<div class="jumbotron">
<div class="row">
<div class="tituloSeccion">
    {% trans %}
        pacientebundle.HistoriaClinica.seccionDatosFiliatorios.titulo
    {% endtrans %}
</div><br>

  <div class="col-sm-6">
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloNombre
        {% endtrans %}
    </span>
    <span class="datos">
        {{paciente.nombre}} {{paciente.apellido}}
    </span><br>
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloDoc
        {% endtrans %}
    </span> 
    <span class="datos">{{paciente.tipoDocumento}} - {{paciente.nroDoc}}</span><br>
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloMovil
        {% endtrans %}
    </span> 
    <span class="datos">{{paciente.telefonoMovil}}</span><br>
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloDomicilio
        {% endtrans %}
    </span>
    <span class="datos">{{paciente.calle}} NÂº {{paciente.numero}} e/ {{paciente.calleEntre1}} y {{paciente.calleEntre2}}, {{paciente.barrio}}, {{paciente.localidad}} - {{paciente.partido}}, {{paciente.pais}}</span><br>
  </div>
  <div class="col-sm-6">
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloSexo
        {% endtrans %}
    </span>
    <span class="datos">
        {% if paciente.isMujer() %}
            {% trans %}
                pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloFem
            {% endtrans %}
        {% else %}
            {% trans %}
                pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloMas
            {% endtrans %}
        {% endif %}
    </span><br>
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloFecNac
        {% endtrans %}
    </span>
    <span class="datos">{{paciente.fechaNacimiento|date("d/m/Y")}}</span><br>
    <span class="aux">
        {% trans %}
            pacientebundle.HistoriaClinica.seccionDatosFiliatorios.subtituloTelFijo
        {% endtrans %}
    </span>
    <span class="datos">{{paciente.telefonoFijo}}</span><br>
  </div>
</div>
</div>